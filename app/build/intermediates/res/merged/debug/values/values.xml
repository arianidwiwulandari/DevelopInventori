    </style>
    <style name="ButtonText">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">#ffffff</item>
    <item name="android:gravity">center</item>
    <item name="android:layout_margin">0dp</item>
    <item name="android:textSize">20dp</item>
    <item name="android:textStyle">bold</item>
    <item name="android:shadowDx">1</item>
    <item name="android:shadowDy">1</item>
    <item name="android:shadowRadius">2</item>
</style>
    <style name="DropDownListView.Sikarinstyle" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_sikarinstyle</item>
    </style>
    <style name="DropDownNav.Sikarinstyle" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_sikarinstyle</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_sikarinstyle</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_sikarinstyle</item>
    </style>
    <style name="PopupMenu.Sikarinstyle" parent="@android:style/Widget.Holo.ListPopupWindow">
