    <style name="ActionBar.Solid.Sikarinstyle" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab_solid_sikarinstyle</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_sikarinstyle</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_sikarinstyle</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Sikarinstyle</item>
    </style>
    <style name="ActionBar.Transparent.Sikarinstyle" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_sikarinstyle</item>
