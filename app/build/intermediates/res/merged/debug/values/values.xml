        <item name="android:actionModeBackground">@drawable/cab_background_top_sikarinstyle</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_sikarinstyle</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Sikarinstyle</item>
        
